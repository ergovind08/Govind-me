#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/govindjha/Desktop/myprofile/node_modules/.pnpm/next-sitemap@3.1.55_@next+env@13.5.6_next@13.5.6_react-dom@18.3.1_react@18.3.1__react@18.3.1_/node_modules/next-sitemap/bin/node_modules:/Users/govindjha/Desktop/myprofile/node_modules/.pnpm/next-sitemap@3.1.55_@next+env@13.5.6_next@13.5.6_react-dom@18.3.1_react@18.3.1__react@18.3.1_/node_modules/next-sitemap/node_modules:/Users/govindjha/Desktop/myprofile/node_modules/.pnpm/next-sitemap@3.1.55_@next+env@13.5.6_next@13.5.6_react-dom@18.3.1_react@18.3.1__react@18.3.1_/node_modules:/Users/govindjha/Desktop/myprofile/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/govindjha/Desktop/myprofile/node_modules/.pnpm/next-sitemap@3.1.55_@next+env@13.5.6_next@13.5.6_react-dom@18.3.1_react@18.3.1__react@18.3.1_/node_modules/next-sitemap/bin/node_modules:/Users/govindjha/Desktop/myprofile/node_modules/.pnpm/next-sitemap@3.1.55_@next+env@13.5.6_next@13.5.6_react-dom@18.3.1_react@18.3.1__react@18.3.1_/node_modules/next-sitemap/node_modules:/Users/govindjha/Desktop/myprofile/node_modules/.pnpm/next-sitemap@3.1.55_@next+env@13.5.6_next@13.5.6_react-dom@18.3.1_react@18.3.1__react@18.3.1_/node_modules:/Users/govindjha/Desktop/myprofile/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../next-sitemap/bin/next-sitemap.cjs" "$@"
else
  exec node  "$basedir/../next-sitemap/bin/next-sitemap.cjs" "$@"
fi
